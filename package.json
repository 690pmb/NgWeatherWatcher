{
  "name": "ng-weather-watcher",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "mobile": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "cordova": "ng build --configuration cordova --base-href file:///android_asset/www/ --progress false",
    "lint": "run-s lint:*",
    "lint:stylelint": "stylelint \"**/*.scss\" verbose",
    "lint:lint": "gts lint",
    "lint:format": "prettier --check .",
    "stylelint-check": "stylelint-config-prettier-check",
    "fix": "run-s fix:*",
    "fix:format": "prettier --write .",
    "fix:stylelint": "stylelint --fix \"**/*.scss\" verbose",
    "fix:fix": "gts fix",
    "clean": "gts clean",
    "compile": "tsc --noEmit",
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky install",
    "prepare:compile": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.5",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.0.5",
    "@angular/compiler": "14.0.5",
    "@angular/core": "14.0.5",
    "@angular/forms": "14.0.5",
    "@angular/material": "14.2.7",
    "@angular/platform-browser": "14.0.5",
    "@angular/platform-browser-dynamic": "14.0.5",
    "@angular/router": "14.0.5",
    "@angular/service-worker": "14.0.5",
    "@fortawesome/angular-fontawesome": "0.11.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "class-transformer": "0.5.1",
    "hammerjs": "2.0.8",
    "jwt-decode": "3.1.2",
    "luxon": "3.0.1",
    "ng2-nouislider": "1.8.3",
    "nouislider": "15.6.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "tslib": "2.4.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "14.0.5",
    "@types/luxon": "2.3.2",
    "@types/node": "14.18.36",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "gts": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "postcss-sass": "0.5.0",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "26.0.0",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.ts": "eslint --max-warnings 0 --fix --no-ignore",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix"
  }
}
