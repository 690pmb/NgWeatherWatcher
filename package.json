{
    "name": "ng-weather-watcher",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "cordova": "ng build --prod --base-href file:///android_asset/www/ --progress false",
        "test": "ng test",
        "lint-staged:format": "prettier --write",
        "lint-staged:stylelint": "stylelint --fix --syntax scss verbose",
        "lint": "run-p lint:**",
        "lint:ng": "ng lint --fix NgWeatherWatcher",
        "lint:format": "yarn lint-staged:format \"./src/**/*.{ts,json,scss,html}\"",
        "lint:stylelint": "yarn lint-staged:stylelint \"src/**/*.scss\" verbose",
        "stylelint-check": "stylelint-config-prettier-check",
        "postinstall": "husky install",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "11.2.5",
        "@angular/cdk": "11.2.0",
        "@angular/common": "11.2.5",
        "@angular/compiler": "11.2.5",
        "@angular/core": "11.2.5",
        "@angular/forms": "11.2.5",
        "@angular/material": "11.2.0",
        "@angular/platform-browser": "11.2.5",
        "@angular/platform-browser-dynamic": "11.2.5",
        "@angular/router": "11.2.5",
        "@fortawesome/angular-fontawesome": "0.8.2",
        "@fortawesome/fontawesome-svg-core": "1.2.36",
        "@fortawesome/free-solid-svg-icons": "5.15.4",
        "@ngx-translate/core": "13.0.0",
        "@ngx-translate/http-loader": "6.0.0",
        "hammerjs": "2.0.8",
        "jwt-decode": "3.1.2",
        "rxjs": "6.6.7",
        "tslib": "2.3.1",
        "zone.js": "0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1102.4",
        "@angular-eslint/builder": "2.1.1",
        "@angular-eslint/eslint-plugin": "2.1.1",
        "@angular-eslint/eslint-plugin-template": "2.1.1",
        "@angular-eslint/schematics": "2.1.1",
        "@angular-eslint/template-parser": "2.1.1",
        "@angular/cli": "11.2.4",
        "@angular/compiler-cli": "11.2.5",
        "@types/node": "16.11.6",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/eslint-plugin-tslint": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "eslint": "8.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-prettier": "3.4.1",
        "husky": "4.3.8",
        "lint-staged": "11.2.6",
        "npm-run-all": "4.1.5",
        "pinst": "2.1.6",
        "prettier": "2.4.1",
        "stylelint": "13.13.1",
        "stylelint-config-prettier": "8.0.2",
        "stylelint-config-sass-guidelines": "8.0.0",
        "stylelint-config-standard": "21.0.0",
        "ts-node": "8.3.0",
        "typescript": "4.4.4"
    },
    "lint-staged": {
        "*.{ts,json,scss,html}": "yarn lint-staged:format",
        "*.scss": "yarn lint-staged:stylelint"
    }
}
