{
  "name": "ng-weather-watcher",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "cordova": "ng build --configuration production --base-href file:///android_asset/www/ --progress false",
    "test": "ng test",
    "lint": "run-s lint:*",
    "lint:stylelint": "stylelint --syntax scss \"src/**/*.scss\" verbose",
    "lint:lint": "gts lint",
    "lint:format": "prettier --check .",
    "stylelint-check": "stylelint-config-prettier-check",
    "fix": "run-s fix:*",
    "fix:stylelint": "stylelint --fix --syntax scss \"src/**/*.scss\" verbose",
    "fix:fix": "gts fix",
    "fix:format": "prettier --write .",
    "e2e": "ng e2e",
    "clean": "gts clean",
    "compile": "tsc --noEmit",
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky install",
    "prepare:compile": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@fortawesome/angular-fontawesome": "0.9.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "jwt-decode": "3.1.2",
    "rxjs": "6.6.7",
    "tslib": "2.4.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.17",
    "@angular/cli": "12.2.17",
    "@angular/compiler-cli": "12.2.16",
    "@types/node": "14.18.21",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "37.9.7",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "gts": "3.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "26.0.0",
    "typescript": "4.2.4"
  },
  "lint-staged": {
    "*.ts": "eslint --max-warnings 0 --fix --no-ignore",
    "*": "prettier --ignore-unknown --write",
    "*.scss": "stylelint --fix --syntax scss"
  }
}
